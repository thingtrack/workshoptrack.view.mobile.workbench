<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @equipmenttype.id %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
    <a href="<%= url_for :action => :edit, :id => @equipmenttype.object %>" class="ui-btn-right" data-icon="star">
      Edit
    </a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">Id</div>
            <div class="itemValue"><%= @equipmenttype.id %></div>
          </li>
      
          <li>
            <div class="itemLabel">Description</div>
            <div class="itemValue"><%= @equipmenttype.description %></div>
          </li>
      
          <li>
            <div class="itemLabel">Code</div>
            <div class="itemValue"><%= @equipmenttype.code %></div>
          </li>
      
          <li>
            <div class="itemLabel">Capacity</div>
            <div class="itemValue"><%= @equipmenttype.capacity %></div>
          </li>
      
          <li>
            <div class="itemLabel">Speed</div>
            <div class="itemValue"><%= @equipmenttype.speed %></div>
          </li>
      
    </ul>
  </div>

</div>
